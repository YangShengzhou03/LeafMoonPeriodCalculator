{
  "app": {
    "signingConfigs": [],
    // 修正1：products → product（单对象，符合官方规范）
    "product": {
      "default": {
        "signingConfig": "default",
        "targetSdkVersion": {
          "platformVersion": "5.0.0",
          "apiVersion": 10
        },
        "compatibleSdkVersion": {
          "platformVersion": "5.0.0",
          "apiVersion": 10
        },
        "compileSdkVersion": {
          "platformVersion": "5.0.0",
          "apiVersion": 10
        },
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        },
        // 关联模块（原modules配置整合到product中）
        "modules": [
          {
            "name": "entry",
            "srcPath": "./entry"
          }
        ]
      }
    }
  },
  // 修正2：移除多余的buildModeSet（非必需，且语法错误）
  // 修正3：简化modules配置（仅保留核心关联）
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    }
  ]
}